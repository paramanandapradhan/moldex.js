{
	"name": "@cloudparker/moldex.js",
	"version": "0.0.75",
	"description": "A Tailwindcss Component Library",
	"license": "MIT",
	"author": "cloudparker.com",
	"keywords": [
		"js",
		"javascript",
		"tailwindcss"
	],
	"repository": "https://github.com/paramanandapradhan/moldex.js",
	"homepage": "https://cloudparker.com",
	"main": "./dist/index.js",
	"scripts": {
		"dev": "vite dev --port 3030",
		"build": "vite build && npm run package",
		"build:tailwind": "npx tailwindcss -i ./src/lib/tailwind.css -o dist/tailwind.css --minify",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"svelte": "^5.0.0-next.1"
	},
	"devDependencies": {
		"@cloudparker/easy-cropperjs-svelte": "^2.4.0",
		"@cloudparker/easy-script-loader-svelte": "^4.0.0",
		"@playwright/test": "^1.46.1",
		"@sveltejs/adapter-auto": "^3.2.4",
		"@sveltejs/kit": "^2.5.24",
		"@sveltejs/package": "^2.3.4",
		"@sveltejs/vite-plugin-svelte": "^4.0.0-next.8",
		"@tailwindcss/aspect-ratio": "^0.4.2",
		"@tailwindcss/forms": "^0.5.7",
		"@types/eslint": "^9.6.1",
		"autoprefixer": "^10.4.20",
		"eslint": "^9.9.1",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.43.0",
		"esm-env": "^1.0.0",
		"globals": "^15.9.0",
		"moment": "^2.30.1",
		"postcss": "^8.4.41",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.6",
		"publint": "^0.2.10",
		"svelte": "^5.0.0-next.264",
		"svelte-check": "^3.8.6",
		"tailwindcss": "^3.4.10",
		"typescript": "^5.5.4",
		"typescript-eslint": "^8.2.0",
		"vite": "^5.4.2",
		"vitest": "^2.0.5"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module"
}